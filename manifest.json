{
  "manifest_version": 2,
  "name": "타바타 타이머",
  "version": "1.0.0",
  "description": "타바타 타이머",
  "icons": {
    "120": "./img/tabata.png"
  },
  "sidebar_action": {
    "default_page": "index.html",
    "default_icon": {
      "16": "./img/tabata.png"
    },
    "default_title": "아!맞다",
    "use_navigation_bar": false
  },
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://kit.fontawesome.com/20fc56bb28.js; object-src 'self'",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  }
}
